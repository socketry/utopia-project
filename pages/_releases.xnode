<p>Please browse the <a href="changes/index">changes</a> for all releases.</p>

<?r
	base = self[:base]
	
	base.releases.first(10).each do |release|
?>
	<section>
		<h3>#{release.name}</h3>
		
		<ul>
			<?r release.summary do |title| ?>
				<li><a href="#{release.href(anchor: title)}">#{title}</a></li>
			<?r end ?>
		</ul>
	</section>
<?r
	end
?>